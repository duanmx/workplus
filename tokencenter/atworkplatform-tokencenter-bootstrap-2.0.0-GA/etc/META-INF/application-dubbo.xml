<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="${dubbo.application}"/>
    <dubbo:protocol name="${dubbo.protocol}" host="${dubbo.protocol.host}" port="${dubbo.protocol.port}"/>
    <dubbo:registry address="${dubbo.registry}"/>

    <dubbo:service interface="io.atworkplatform.tokencenter.api.TicketService" retries="0" ref="ticketService"/>
    <dubbo:service interface="io.atworkplatform.tokencenter.api.TokenService" retries="0" ref="tokenService"/>
</beans>